class Mobil:
    def __init__(self, merk, tahun, warna):
        self.merk = merk
        self.tahun = tahun
        self.warna = warna

    def info(self):
        return f"Mobil {self.merk} tahun {self.tahun}, berwarna {self.warna}"

class Mahasiswa:
    def __init__(self, nama, npm, jurusan):
        self.nama = nama
        self.npm = npm
        self.jurusan = jurusan

    def info(self):
        return f"Mahasiswa {self.nama} ({self.npm}), jurusan {self.jurusan}"

# Pembuatan objek dari class Mobil
mobil1 = Mobil("Honda Civic", 2021, "Merah")
mobil2 = Mobil("Toyota Fortuner", 2022, "Putih")

# Memanggil method info() dari objek mobil1 dan mobil2
print(mobil1.info())  # Output: Mobil Honda Civic tahun 2021, berwarna Merah
print(mobil2.info())  # Output: Mobil Toyota Fortuner tahun 2022, berwarna Putih

# Pembuatan objek dari class Mahasiswa
mahasiswa1 = Mahasiswa("Radit", "202110001", "Teknik Informatika")
mahasiswa2 = Mahasiswa("Dela", "202110002", "Ekonomi")

# Memanggil method info() dari objek mahasiswa1 dan mahasiswa2
print(mahasiswa1.info())  # Output: Mahasiswa Radit (202110001), jurusan Teknik Informatika
print(mahasiswa2.info())  # Output: Mahasiswa Dela (202110002), jurusan Ekonomi
